{
	"name": "auth-example",
	"private": true,
	"description": "An example project for Auth.js with SvelteKit",
	"repository": "https://github.com/nextauthjs/sveltekit-auth-example",
	"bugs": {
		"url": "https://github.com/nextauthjs/next-auth/issues"
	},
	"homepage": "https://sveltekit-auth-example.vercel.app",
	"scripts": {
		"dev": "vite dev --port 3002",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
		"format": "prettier --write --ignore-path=../../.prettierignore ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.11.0",
		"svelte": "3.55.0",
		"svelte-check": "2.10.2",
		"typescript": "4.9.4",
		"vite": "4.0.1"
	},
	"dependencies": {
		"@auth/core": "0.5.1",
		"@auth/sveltekit": "0.3.0"
	},
	"type": "module"
}
